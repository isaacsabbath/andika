{
  "builds": [
    {
      "src": "blogfast/wsgi.py",
      "use": "@vercel/python",
      "config" : { 
        "maxLambdaSize": "15mb" ,
        "runtime": "python3.9",
        "buildCommand": "pip install -r blogfast/requirements.txt"
    }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "blogfast/wsgi.py"
    }
  ]
}
